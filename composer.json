{
  "name": "tourze/phpunit-workerman",
  "description": "Workerman PHPUnit Support",
  "license": "MIT",
  "require": {
    "ext-sockets": "*",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/yaml": "^7.3",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\PHPUnitWorkerman\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\PHPUnitWorkerman\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}